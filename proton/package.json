{
  "name": "Proton",
  "version": "1.0.0",
  "description": "All yours app in one app",
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./public ./src --ext js",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && cross-env API_URL=http://localhost:3000/api electron .\"",
    "dist-dev": "yarn build && node dist.js",
    "dist": "yarn build && cross-env NODE_ENV=production node dist.js",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/webcatalog/proton.git",
  "author": "WebCatalog <support@webcatalog.io>",
  "license": "MPL-2.0",
  "private": true,
  "devDependencies": {
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "classnames": "2.2.5",
    "concurrently": "3.5.0",
    "cross-env": "5.1.0",
    "electron": "1.7.9",
    "electron-builder": "19.37.2",
    "electron-builder-squirrel-windows": "19.39.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "fs-extra": "4.0.2",
    "material-ui": "1.0.0-beta.17",
    "material-ui-icons": "1.0.0-beta.17",
    "mocha": "3.5.3",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-scripts": "1.0.14",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "spectron": "3.7.2",
    "typeface-roboto": "0.0.41",
    "wait-on": "2.0.2"
  },
  "dependencies": {
    "electron-is-dev": "0.3.0",
    "electron-updater": "2.13.0",
    "electron-window-state": "4.1.1",
    "yargs-parser": "8.0.0"
  },
  "homepage": "./",
  "proxy": "https://webcatalog.io"
}
