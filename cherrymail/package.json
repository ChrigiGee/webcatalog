{
  "name": "CherryMail",
  "description": "Email App",
  "version": "1.0.1",
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "npx electron-builder install-app-deps",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./public ./src --ext js",
    "lint:fix": "eslint ./public ./src --ext js --fix",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "dist": "yarn build && cross-env NODE_ENV=production node dist.js",
    "dist-dev": "yarn build && node dist.js",
    "test": "mocha ./tests"
  },
  "repository": "https://github.com/quanglam2807/cherrymail",
  "author": "Quang Lam <quang.lam2807@gmail.com>",
  "dependencies": {
    "electron-is-dev": "1.1.0",
    "electron-settings": "3.2.0",
    "electron-spellchecker": "1.1.2",
    "electron-updater": "4.0.6",
    "electron-window-state": "5.0.3",
    "menubar": "5.2.3",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "blueimp-md5": "2.10.0",
    "classnames": "2.2.6",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "electron": "4.1.5",
    "electron-builder": "20.39.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "lodash.camelcase": "4.3.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "6.0.1",
    "react-scripts": "3.0.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "0.0.54",
    "wait-on": "3.2.0"
  },
  "homepage": "./",
  "private": true,
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
