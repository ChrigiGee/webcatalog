{
  "name": "Appifier",
  "version": "9.4.0",
  "description": "Appifier helps you turn web apps into real apps.",
  "main": "public/electron.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps && node hacky.js",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./dist.js ./hacky.js ./public ./src --ext js",
    "lint-all": "eslint ./dist.js ./hacky.js ./public ./src ./packages/appifier/src ./packages/appifier/app/main ./packages/appifier/app/src  --ext js",
    "start": "react-scripts start",
    "electron-dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && cross-env API_URL=http://localhost:3000/api electron .\"",
    "dist-dev": "yarn build && node dist.js",
    "dist": "yarn build && cross-env NODE_ENV=production node dist.js",
    "test": "mocha ./tests",
    "appifier:install": "cd packages/appifier && yarn",
    "appifier:build": "cd packages/appifier && yarn build",
    "appifier:test": "cd packages/appifier && yarn test",
    "appifier:publish": "cd packages/appifier && npm publish"
  },
  "repository": "https://github.com/quanglam2807/appifier",
  "author": {
    "name": "Quang Lam",
    "email": "quang.lam2807@gmail.com",
    "url": "https://quang.im"
  },
  "license": "MPL-2.0",
  "dependencies": {
    "appifier": "9.4.0",
    "electron-is-dev": "0.3.0",
    "electron-settings": "3.1.4",
    "follow-redirects": "1.3.0",
    "fs-extra": "5.0.0",
    "is-url": "1.2.2",
    "tmp": "0.0.33",
    "yargs-parser": "8.1.0"
  },
  "devDependencies": {
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chalk": "2.3.0",
    "classnames": "2.2.5",
    "concurrently": "3.5.1",
    "cross-env": "5.1.3",
    "electron": "1.8.2",
    "electron-builder": "19.52.1",
    "electron-builder-squirrel-windows": "19.53.3",
    "electron-rebuild": "1.6.0",
    "eslint": "4.15.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.3.0",
    "material-ui": "1.0.0-beta.25",
    "material-ui-icons": "1.0.0-beta.17",
    "mocha": "4.1.0",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.6",
    "react-scripts": "1.0.17",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "replace-in-file": "3.1.0",
    "request": "2.83.0",
    "semver": "5.4.1",
    "spectron": "3.7.2",
    "typeface-roboto": "0.0.50",
    "wait-on": "2.1.0"
  },
  "homepage": "./",
  "private": true
}
